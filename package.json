{
  "name": "sample-ts-express",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "tsc --build --watch",
    "serve": "ts-node -r tsconfig-paths/register ./dist/bin/www.js",
    "start": "nodemon --watch src/ -e ts --exec 'tsc && npm run serve'",
    "lint": "eslint './src/**/*.ts'"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv-flow": "^3.2.0",
    "express": "~4.16.1",
    "hbs": "^4.1.2",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/debug": "^4.1.7",
    "@types/dotenv-flow": "^3.2.0",
    "@types/express": "^4.17.13",
    "@types/http-errors": "^1.8.1",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.6.1",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.24.1",
    "nodemon": "^2.0.12",
    "ts-node": "^10.2.0",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.3.5"
  }
}
